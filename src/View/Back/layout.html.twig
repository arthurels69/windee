<!doctype html>
<link rel="stylesheet" href="/assets/css/style.css">
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}{% endblock %} </title>






    {% block stylesheet %}
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

        <link rel="stylesheet" href="/assets/css/style.css">

        <link rel="icon" href="/assets/images/favicon.png">
        <link rel="stylesheet" href="/assets/css/style.css">

    {% endblock %}


</head>


<body>
{% block nav %}
    <nav class="textenav navbar navbar-dark navbar-expand-lg " style="background: cornflowerblue">
        <a class="navbar-brand text-white" href="/back/index">Windee</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">

                    <a class="nav-link text-white" href="/back/index">Stations</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-white" href="/back/booking">Réservation</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-white" href="/vehicle/vehicle">Véhicule</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-white" href="/back/logout">{% if session.user is defined %}Déconnexion{% endif %}</a>
                </li>

            </ul>
        </div>
    </nav>
{% endblock  %}


{% block content %}
{% endblock %}
{% block footer %}
    <footer class="page-footer font-small blue py-4 mt-4">

        <!-- Footer Links -->
        <div class="footer-a container text-center ">
          <a class="text-white" href="/home/index">Lien vers Windee</a>
        </div>
    </footer>



{% endblock %}
{% block javascript %}
<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="https://use.fontawesome.com/236586708f.js"></script>


{% endblock %}
</body>
</html>



